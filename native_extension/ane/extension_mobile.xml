<?xml version="1.0" encoding="utf-8"?>
<extension xmlns="http://ns.adobe.com/air/extension/19.0">
    <id>com.tuarua.BatteryANE</id>
	<name>Battery ANE</name>
	<copyright>This work is licensed under Apache License, Copyright (c) 2018 Tua Rua Ltd.</copyright>
	<versionNumber>0.0.5</versionNumber>
    <platforms>
		<platform name="Android-ARM"> 
           <applicationDeployment>
				<nativeLibrary>classes.jar</nativeLibrary>
				<initializer>com.tuarua.BatteryANE</initializer>
				<finalizer>com.tuarua.BatteryANE</finalizer>
			</applicationDeployment>
        </platform>
		<platform name="Android-x86">
           <applicationDeployment>
				<nativeLibrary>classes.jar</nativeLibrary>
				<initializer>com.tuarua.BatteryANE</initializer>
				<finalizer>com.tuarua.BatteryANE</finalizer>
			</applicationDeployment>
        </platform>
        <platform name="iPhone-ARM">
            <applicationDeployment>
                <nativeLibrary>libBatteryANE.a</nativeLibrary>
                <initializer>TRBATExtInizer</initializer>
                <finalizer>TRBATExtFinizer</finalizer>
            </applicationDeployment>
        </platform>
        <platform name="iPhone-x86">
            <applicationDeployment>
                <nativeLibrary>libBatteryANE.a</nativeLibrary>
                <initializer>TRBATExtInizer</initializer>
                <finalizer>TRBATExtFinizer</finalizer>
            </applicationDeployment>
        </platform>
        <platform name="default">
            <applicationDeployment/>
        </platform>
    </platforms>
</extension>
